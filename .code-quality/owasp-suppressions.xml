<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    
    <!-- Suppress false positives for Quarkus dependencies -->
    <suppress>
        <notes><![CDATA[
        Quarkus framework dependencies - managed by platform BOM
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/io\.quarkus/.*$</packageUrl>
        <cve>CVE-2021-44228</cve> <!-- Log4j - not used in Quarkus -->
    </suppress>
    
    <!-- Suppress test-only dependencies -->
    <suppress>
        <notes><![CDATA[
        Test dependencies - not included in production
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.junit\..*$</packageUrl>
        <cve>.*</cve>
    </suppress>
    
    <suppress>
        <notes><![CDATA[
        Mockito test framework - not included in production
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.mockito/.*$</packageUrl>
        <cve>.*</cve>
    </suppress>
    
    <!-- Suppress development-only dependencies -->
    <suppress>
        <notes><![CDATA[
        Development and build-time dependencies
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.apache\.maven\.plugins/.*$</packageUrl>
        <cve>.*</cve>
    </suppress>
    
    <!-- Example: Suppress specific CVE if it's a false positive -->
    <!--
    <suppress>
        <notes><![CDATA[
        False positive - this vulnerability doesn't apply to our usage
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.example/vulnerable-lib@.*$</packageUrl>
        <cve>CVE-2023-12345</cve>
    </suppress>
    -->
    
</suppressions>
